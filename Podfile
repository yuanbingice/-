# platform :ios, '9.0'

target 'douyuDemo' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!


  pod "SnapKit", "~> 0.0"
  
  pod "Alamofire", "~> 3.0"
  pod "SwiftyJSON", "~> 2.0"
  
  pod "YYKit"
  #循环滚动的滚动视图
  pod "SDCycleScrollView"
  
  
  #方便我们去设置 AttributedString // 简单的图文混排,设置文字属性,设置 label 某些文字的点击事件,可以加载网络图片
  pod "WPAttributedMarkup"
  

  # Pods for douyuDemo

end
